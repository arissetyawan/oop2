   $(document).ready(function () {
      getRightMenu();
      function getRightMenu(){
         $.ajax({
            url: baseurl + "questions/ajax/ajaxGetRightMenu.php",
            method: "POST",
            success: function (data) {
            $('#contribution_menu').html(data);
            }
         });
      }
            getLatestQuestions();
         function getLatestQuestions() {
            $.ajax({
               url: baseurl + "questions/ajax/ajaxGetLatestQuestions.php",
               method: "POST",
               success: function (data) {
               $('#latest_questions').html(data);
               }
            });
         }
          });

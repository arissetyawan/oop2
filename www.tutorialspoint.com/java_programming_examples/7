                                        <h2>  
                                        <a href='/programming_example/fgjrFj/java-lang-thread-currentthread'>java.lang.Thread.currentThread()</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/java_programming_examples/'><span>java</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package com.tutorialspoint;

import java.lang.*;

public class ThreadDemo implements Runnable {

   ThreadDemo() {
      // main thread
      Thread currThread = Thread.currentThread();
      
      // thread created
      Thread t = new Thread(this, &quot;Admin Thread&quot;);
   
      System.out.println(&quot;current thread = &quot; + currThread);
      System.out.println(&quot;thread created = &quot; + t);
      
      // this will call run() function
      t.start();
   }

   public void run() {
      System.out.println(&quot;This is run() method&quot;);
   }

   public static void main(String args[]) {
      new ThreadDemo();
   }
} </pre>
                                       </p>

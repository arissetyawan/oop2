                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/rzHMbf/java-lang-system-setproperty'>java.lang.System.setProperty()</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/java_programming_examples/'><span>java</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package com.tutorialspoint;

import java.lang.*;

public class SystemDemo {

   public static void main(String[] args) {

      // prints Java Runtime Version before property set
      System.out.print(&quot;Previous : &quot;);
      System.out.println(System.getProperty(&quot;java.runtime.version&quot; ));
      System.setProperty(&quot;java.runtime.version&quot;, &quot;Java Runtime 1.6.0&quot;);
     
      // prints Java Runtime Version after property set
      System.out.print(&quot;New : &quot;);
      System.out.println(System.getProperty(&quot;java.runtime.version&quot; ));
   }
} </pre>
                                       </p>

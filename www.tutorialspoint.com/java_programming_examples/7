                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/bfXFM5/java-lang-system-identityhashcode'>java.lang.System.identityHashCode()</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/java_programming_examples/'><span>java</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package com.tutorialspoint;

import java.lang.*;
import java.io.*;

public class SystemDemo {

   public static void main(String[] args) throws Exception {

      File file1 = new File(&quot;amit&quot;);
      File file2 = new File(&quot;amit&quot;);
      File file3 = new File(&quot;ansh&quot;);

      // returns the HashCode
      int ret1 = System.identityHashCode(file1);
      System.out.println(ret1);

      // returns different HashCode for same filename
      int ret2 = System.identityHashCode(file2);
      System.out.println(ret2);

      // returns the HashCode    
      int ret3 = System.identityHashCode(file3);
      System.out.println(ret3);
   }
} </pre>
                                       </p>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/u5BMjS/java-lang-system-maplibraryname'>java.lang.System.mapLibraryName()</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/java_programming_examples/'><span>java</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package com.tutorialspoint;

import java.lang.*;

public class SystemDemo {

   public static void main(String[] args) {

      // prints the name of the Operating System
      System.out.println(System.getProperty(&quot;os.name&quot;));

      /* maps a library name into a platform-specific string representing
         a native library */
      String str = System.mapLibraryName(&quot;os.name&quot;);   
      System.out.println(str);
   }
} </pre>
                                       </p>

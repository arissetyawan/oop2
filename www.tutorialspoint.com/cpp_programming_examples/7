                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/XRrE29/cplusplus-check-if-it-is-a-sparse-matrix-or-not'>C++ check if it is a sparse matrix or not</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/cpp_programming_examples/'><span>cpp</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">#include&lt;iostream&gt;
using namespace std;

int main () {
   int a[10][10] = { {2, 0, 0} , {0, 3, 8} , {0, 9, 0} };
   int i, j, count = 0;
   int r = 3, c = 3; 
 
   for (i = 0; i &lt; r; ++i) {
      for (j = 0; j &lt; c; ++j) {
         if (a[i][j] == 0)
         count++;
      }
   }
    
   cout&lt;&lt;&quot;The matrix is:&quot;&lt;&lt;endl;
    
   for (i = 0; i &lt; r; ++i) {
      for (j = 0; j &lt; c; ++j) {
         cout&lt;&lt;a[i][j]&lt;&lt;&quot; &quot;;
      }
      cout&lt;&lt;endl;
   }
    
   cout&lt;&lt;&quot;There are &quot;&lt;&lt;count&lt;&lt;&quot; zeros in the matrix&quot;&lt;&lt;endl;
    
   if (count &gt; ((r * c)/ 2))
   cout&lt;&lt;&quot;This is a sparse matrix&quot;&lt;&lt;endl;
   else
   cout&lt;&lt;&quot;This is not a sparse matrix&quot;&lt;&lt;endl;
   
   return 0;
}</pre>
                                       </p>

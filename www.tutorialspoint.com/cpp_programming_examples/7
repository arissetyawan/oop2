                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/QXIjaD/cplusplus-multiplies-two-matrices-by-passing-the-matrices-to-functions'>C++ multiplies two matrices by passing the matrices to functions</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/cpp_programming_examples/'><span>cpp</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">#include&lt;iostream&gt;
using namespace std;

void MatrixMultiplication(int a[2][3],int b[3][3]) {   
   int product[10][10], r1=2, c1=3, r2=3, c2=3, i, j, k;
    
   if (c1 != r2) {
      cout&lt;&lt;&quot;Column of first matrix should be equal to row of second matrix&quot;;
   } else {  
      cout&lt;&lt;&quot;The first matrix is:&quot;&lt;&lt;endl;
 
      for(i=0; i&lt;r1; ++i) {  
         for(j=0; j&lt;c1; ++j)
            cout&lt;&lt;a[i][j]&lt;&lt;&quot; &quot;;
        
            cout&lt;&lt;endl;
      }
     
      cout&lt;&lt;endl;

      cout&lt;&lt;&quot;The second matrix is:&quot;&lt;&lt;endl;
 
      for(i=0; i&lt;r2; ++i) {  
         for(j=0; j&lt;c2; ++j)
            cout&lt;&lt;b[i][j]&lt;&lt;&quot; &quot;;
        
            cout&lt;&lt;endl;
      }
    
      cout&lt;&lt;endl;
    
      for(i=0; i&lt;r1; ++i)
         for(j=0; j&lt;c2; ++j) {
            product[i][j] = 0;
         }

         for(i=0; i&lt;r1; ++i)
            for(j=0; j&lt;c2; ++j)
               for(k=0; k&lt;c1; ++k) {
                  product[i][j]+=a[i][k]*b[k][j];
               }

         cout&lt;&lt;&quot;Product of the two matrices is:&quot;&lt;&lt;endl;
    
         for(i=0; i&lt;r1; ++i) {   
            for(j=0; j&lt;c2; ++j)
               cout&lt;&lt;product[i][j]&lt;&lt;&quot; &quot;;
               cout&lt;&lt;endl;
         }
   }
}

int main() {
   int a[2][3] = { {2, 4, 1} , {2, 3, 9} };
   int b[3][3] = { {1, 2, 3} , {3, 6, 1} , {2, 9, 7} };
    
   MatrixMultiplication(a,b);
    
   return 0;
}</pre>
                                       </p>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/DeFReS/cplusplus-calculate-the-standard-deviation'>C++ calculate the standard deviation</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/cpp_programming_examples/'><span>cpp</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">#include &lt;iostream&gt;
#include &lt;cmath&gt;
using namespace std;

int main() {
   float val[5] = {12.5, 7.0, 10.0, 7.8, 15.5};
   float sum = 0.0, mean, variance = 0.0, stdDeviation;
   int i;

   for(i = 0; i &lt; 5; ++i)
   sum += val[i];

   mean = sum/5;

   for(i = 0; i &lt; 5; ++i)
   variance += pow(val[i] - mean, 2);

   variance=variance/5;
    
   stdDeviation = sqrt(variance);
    
   cout&lt;&lt;&quot;The data values are: &quot;;
    
   for(i = 0; i &lt; 5; ++i)
      cout&lt;&lt; val[i] &lt;&lt;&quot; &quot;;
    
      cout&lt;&lt;endl;

      cout&lt;&lt;&quot;The standard deviation of these data values is &quot;&lt;&lt;stdDeviation;    
}</pre>
                                       </p>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/zMExRL/angular-material-progress-bars'>Angular Material Progress Bars</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/angularjs_programming_examples/'><span>angularjs</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">&lt;html lang = &quot;en&quot;&gt;
   &lt;head&gt;
      &lt;link rel = &quot;stylesheet&quot;
         href = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css&quot;&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js&quot;&gt;&lt;/script&gt;
      &lt;link rel = &quot;stylesheet&quot; href = &quot;https://fonts.googleapis.com/icon?family=Material+Icons&quot;&gt;
      
      &lt;script language = &quot;javascript&quot;&gt;
         angular
            .module('firstApplication', ['ngMaterial'])
            .controller('progressbarsController', progressbarsController);

         function progressbarsController ($scope, $interval) {
            var self = this,  j = 0, counter = 0;
            self.modes = [ ];
            self.activated = true;
            self.determinateValue = 30;
            
            self.toggleActivation = function() {
               if ( !self.activated ) self.modes = [ ];
               if (  self.activated ) j = counter = 0;
            };
            
            $interval(function() {
               self.determinateValue += 1;

               if (self.determinateValue &gt; 100) {
                  self.determinateValue = 30;
               }
               
               if ( (j &lt; 5) &amp;&amp; !self.modes[j] &amp;&amp; self.activated ) {
                  self.modes[j] = 'indeterminate';
               }
               if ( counter++ % 4 == 0 ) j++;
            }, 100, 0, true);
         }
      &lt;/script&gt;      
   &lt;/head&gt;
   
   &lt;body ng-app = &quot;firstApplication&quot;&gt; 
      &lt;div id = &quot;progressbarsContainer&quot; ng-controller = &quot;progressbarsController as ctrl&quot;
         layout = &quot;column&quot; ng-cloak&gt;
         
         &lt;h4 style = &quot;margin-top:10px&quot;&gt;Determinate &lt;/h4&gt;&lt;p&gt;&lt;/p&gt;
         &lt;div layout = &quot;row&quot; layout-sm = &quot;column&quot; layout-align = &quot;space-around&quot;&gt;
            &lt;md-progress-circular md-mode = &quot;determinate&quot;
               value = &quot;{{ctrl.determinateValue}}&quot;&gt;&lt;/md-progress-circular&gt;
         &lt;/div&gt;
         
         &lt;h4 style = &quot;margin-top:10px&quot;&gt;Indeterminate &lt;/h4&gt;
         &lt;div layout = &quot;row&quot; layout-sm = &quot;column&quot; layout-align = &quot;space-around&quot;&gt;
            &lt;md-progress-circular md-mode = &quot;indeterminate&quot;&gt;&lt;/md-progress-circular&gt;
         &lt;/div&gt;
         
         &lt;h4 style = &quot;margin-top:10px&quot;&gt;Theme Based&lt;/h4&gt;
         &lt;div layout = &quot;row&quot; layout-sm = &quot;column&quot; layout-align = &quot;space-around&quot;&gt;
            &lt;md-progress-circular class = &quot;md-hue-2&quot; md-mode = &quot;{{ctrl.modes[0]}}&quot; 
               md-diameter = &quot;20px&quot;&gt;&lt;/md-progress-circular&gt;
            
            &lt;md-progress-circular class = &quot;md-accent&quot; md-mode = &quot;{{ctrl.modes[1]}}&quot;
               md-diameter = &quot;40&quot;&gt;&lt;/md-progress-circular&gt;
            
            &lt;md-progress-circular class = &quot;md-accent md-hue-1&quot; md-mode = &quot;{{ctrl.modes[2]}}&quot;
               md-diameter = &quot;60&quot;&gt;&lt;/md-progress-circular&gt;
            
            &lt;md-progress-circular class = &quot;md-warn md-hue-3&quot; md-mode = &quot;{{ctrl.modes[3]}}&quot;
               md-diameter = &quot;70&quot;&gt;&lt;/md-progress-circular&gt;
            
            &lt;md-progress-circular md-mode = &quot;{{ctrl.modes[4]}}&quot; md-diameter = &quot;96&quot;&gt;
               &lt;/md-progress-circular&gt;
         &lt;/div&gt;
         
         &lt;hr ng-class = &quot;{'visible' : ctrl.activated}&quot;&gt;
         &lt;div id = &quot;loaders&quot; layout = &quot;row&quot; layout-align = &quot;start center&quot;&gt;
            &lt;p&gt;Progress Circular Indicators:    &lt;/p&gt;
            &lt;h5&gt;Off&lt;/h5&gt;
            &lt;md-switch
               ng-model = &quot;ctrl.activated&quot;
               ng-change = &quot;ctrl.toggleActivation()&quot;
               aria-label = &quot;Toggle Progress Circular Indicators&quot;&gt;
               &lt;h5&gt;On&lt;/h5&gt;
            &lt;/md-switch&gt;
         &lt;/div&gt;
         
      &lt;/div&gt;
   &lt;/body&gt;
&lt;/html&gt;</pre>
                                       </p>
                                    </div>
                                    <div class="clear"></div>
                                    <div class="qa_social_links"> 
                                       <div class="">
                                       <div class="qa_links">                                           
                                          <a href="http://tpcg.io/zMExRL" target="_blank" rel="nofollow" class="demo"><button type="button" class="question_btn"><i class="fa fa-external-link"></i> Live Demo</button></a> 
                                                                                    <button type="button" class="btnEmbed question_btn"><i class="fa fa-code"></i> Embed URL </button> 
                                          <div style="display:none;" class="divEmbed arrow_boxx embed-box">
                                             <input type="text" value="http://tpcg.io/zMExRL" />
                                          </div>
                                       </div>
                                       </div>
                                    </div>
                                    <div class="clear"></div>
                                </div>
                            </div> 
                            <div class="clear"></div>
                        </div>      
                                                <div class="clsQuestionSection">      
                            <div class="">   
                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/BPUFr9/angular-material-menu-bar'>Angular Material Menu Bar</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/angularjs_programming_examples/'><span>angularjs</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">&lt;html lang = &quot;en&quot;&gt;
   &lt;head&gt;
      &lt;link rel = &quot;stylesheet&quot;
         href = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css&quot;&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js&quot;&gt;&lt;/script&gt;
      &lt;link rel = &quot;stylesheet&quot; href = &quot;https://fonts.googleapis.com/icon?family=Material+Icons&quot;&gt;
      
      &lt;script language = &quot;javascript&quot;&gt;
         angular
            .module('firstApplication', ['ngMaterial'])
            .controller('menubarController', menubarController);

         function menubarController ($scope, $mdDialog) {
            this.sampleAction = function(name, ev) {
               $mdDialog.show($mdDialog.alert()
                  .title(name)   
                  .textContent('Start learning &quot;' + name + '!')
                  .ok('OK')
                  .targetEvent(ev)
               );
            };
         }                 
      &lt;/script&gt;      
   &lt;/head&gt;
   
   &lt;body ng-app = &quot;firstApplication&quot;&gt; 
      &lt;div id = &quot;menubarContainer&quot; ng-controller = &quot;menubarController as ctrl&quot;
         layout = &quot;row&quot; ng-cloak&gt;
         &lt;md-toolbar class = &quot;md-menu-toolbar&quot;&gt;
            &lt;div layout = &quot;row&quot;&gt;
               &lt;div&gt;
                  &lt;h2 class = &quot;md-toolbar-tools&quot;&gt;Learn @TutorialsPoint&lt;/h2&gt;
                  &lt;md-menu-bar&gt;
                     &lt;md-menu&gt;
                        &lt;button ng-click = &quot;$mdOpenMenu()&quot;&gt;Tutorials&lt;/button&gt;
                        &lt;md-menu-content&gt;
                           
                           &lt;md-menu-item&gt;
                              &lt;md-button ng-click = &quot;ctrl.sampleAction('share', $event)&quot;&gt;Share...&lt;/md-button&gt;
                           &lt;/md-menu-item&gt;
                           
                           &lt;md-menu-divider&gt;&lt;/md-menu-divider&gt;
                           &lt;md-menu-item&gt;
                              &lt;md-menu&gt;
                                 &lt;md-button
                                    ng-click = &quot;$mdOpenMenu()&quot;&gt;Learn&lt;/md-button&gt;
                                 &lt;md-menu-content&gt;
                                    &lt;md-menu-item&gt;
                                       &lt;md-button
                                       ng-click = &quot;ctrl.sampleAction('HTML5', $event)&quot;&gt;
                                       HTML5&lt;/md-button&gt;
                                    &lt;/md-menu-item&gt;
                                    
                                    &lt;md-menu-item&gt;
                                       &lt;md-button
                                          ng-click = &quot;ctrl.sampleAction('jQuery', $event)&quot;&gt;
                                          jQuery&lt;/md-button&gt;
                                    &lt;/md-menu-item&gt;
                                    
                                    &lt;md-menu-item&gt;
                                       &lt;md-button
                                          ng-click = &quot;ctrl.sampleAction('AngularJS', $event)&quot;&gt;
                                          AngularJS&lt;/md-button&gt;
                                    &lt;/md-menu-item&gt;
                                    
                                    &lt;md-menu-item&gt;
                                       &lt;md-button disabled = &quot;disabled&quot;
                                          ng-click = &quot;ctrl.sampleAction('AngularJS 2.0', $event)&quot;&gt;
                                          AngularJS 2.0&lt;/md-button&gt;
                                    &lt;/md-menu-item&gt;
                                    
                                    &lt;md-menu-divider&gt;&lt;/md-menu-divider&gt;
                                    &lt;md-menu-item&gt;
                                       &lt;md-button
                                          ng-click = &quot;ctrl.sampleAction('CSS', $event)&quot;&gt;
                                          CSS&lt;/md-button&gt;
                                    &lt;/md-menu-item&gt;
                                 &lt;/md-menu-content&gt;
                              &lt;/md-menu&gt;
                           &lt;/md-menu-item&gt;
                           
                        &lt;/md-menu-content&gt;
                     &lt;/md-menu&gt;
                  &lt;/md-menu-bar&gt;
               &lt;/div&gt;
            &lt;/div&gt;
         &lt;/md-toolbar&gt;	 		 

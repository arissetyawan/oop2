                                        <h2>  
                                        <a href='/programming_example/s2IX0r/angular-material-selects'>Angular Material Selects</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/angularjs_programming_examples/'><span>angularjs</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">&lt;html lang = &quot;en&quot;&gt;
   &lt;head&gt;
      &lt;link rel = &quot;stylesheet&quot;
         href = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css&quot;&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js&quot;&gt;&lt;/script&gt;
      &lt;link rel = &quot;stylesheet&quot; href = &quot;https://fonts.googleapis.com/icon?family=Material+Icons&quot;&gt;
      
      &lt;script language = &quot;javascript&quot;&gt;
         angular
            .module('firstApplication', ['ngMaterial'])
            .controller('selectController', selectController);

         function selectController ($scope) {
            $scope.users = [
               { id: 1, name: 'Mahesh' },
               { id: 2, name: 'Ramesh' },
               { id: 3, name: 'Sohan' }
            ];
            
            $scope.selectedUser = { id: 2, name: 'Ramesh' };
            $scope.drinks = [
               { category: 'hard', name: 'Beer' },
               { category: 'hard', name: 'Rum' },
               { category: 'hard', name: 'Vodka' },
               { category: 'hard', name: 'Whiski' },        
               { category: 'soft', name: 'Coke' },
               { category: 'soft', name: 'Pepsi' },
               { category: 'soft', name: 'Pulp Maid' },
               { category: 'soft', name: 'Real Juice' }
            ];
         }	  
      &lt;/script&gt;      
   &lt;/head&gt;
   
   &lt;body ng-app = &quot;firstApplication&quot;&gt; 
      &lt;div id = &quot;selectContainer&quot; ng-controller = &quot;selectController as ctrl&quot;
         layout = &quot;row&quot; ng-cloak&gt;
         &lt;md-input-container&gt;
            &lt;label&gt;User&lt;/label&gt;
            &lt;md-select ng-model = &quot;selectedUser&quot;&gt;
               &lt;md-option ng-value = &quot;user&quot;
                  ng-repeat = &quot;user in users&quot;&gt;{{ user.name }}&lt;/md-option&gt;
            &lt;/md-select&gt;
         &lt;/md-input-container&gt;
         
         &lt;md-input-container&gt;
            &lt;label&gt;Drinks&lt;/label&gt;
            &lt;md-select ng-model = &quot;drink&quot;&gt;
               &lt;md-optgroup label = &quot;Hard Drinks&quot;&gt;
                  &lt;md-option ng-value = &quot;drink.name&quot;
                     ng-repeat = &quot;drink in drinks | filter: {category: 'hard' }&quot;&gt;
                     {{drink.name}}&lt;/md-option&gt;
               &lt;/md-optgroup&gt;
               
               &lt;md-optgroup label = &quot;Soft Drinks&quot;&gt;
                  &lt;md-option ng-value = &quot;drink.name&quot;
                     ng-repeat = &quot;drink in drinks | filter: {category: 'soft' }&quot;&gt;
                     {{drink.name}}&lt;/md-option&gt;
               &lt;/md-optgroup&gt;
            &lt;/md-select&gt;
         &lt;/md-input-container&gt;
         
      &lt;/div&gt;
   &lt;/body&gt;
&lt;/html&gt;</pre>

                                                <div class="clsQuestionSection">      
                            <div class="">   
                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/OCEBsA/angular-material-datepicker'>Angular Material DatePicker</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/angularjs_programming_examples/'><span>angularjs</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">&lt;html lang = &quot;en&quot;&gt;
   &lt;head&gt;
      &lt;link rel = &quot;stylesheet&quot;
         href = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css&quot;&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js&quot;&gt;&lt;/script&gt;
      &lt;script src = &quot;https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js&quot;&gt;&lt;/script&gt;
      &lt;style&gt;
         .datepickerdemo md-content {
            padding-bottom: 200px; 
         }
         
        .datepickerdemo .validation-messages {
            font-size: 11px;
            color: darkred;
            margin: 10px 0 0 25px; 
         }
      &lt;/style&gt;
      
      &lt;script language = &quot;javascript&quot;&gt;
         angular
            .module('firstApplication', ['ngMaterial','ngMessages'])
            .controller('dateController', dateController);

         function dateController ($scope) {
            $scope.myDate = new Date();
            $scope.minDate = new Date(
               $scope.myDate.getFullYear(),
               $scope.myDate.getMonth() - 2,
               $scope.myDate.getDate());
               
            $scope.maxDate = new Date(
               $scope.myDate.getFullYear(),
               $scope.myDate.getMonth() + 2,
               $scope.myDate.getDate());
               
            $scope.onlyWeekendsPredicate = function(date) {
               var day = date.getDay();
               return day === 0 || day === 6;
            }
         }                 
      &lt;/script&gt;    
   &lt;/head&gt;
   
   &lt;body ng-app = &quot;firstApplication&quot;&gt; 
      &lt;div class = &quot;datepickerdemo&quot; ng-controller = &quot;dateController as ctrl&quot;
         layout = &quot;column&quot; ng-cloak&gt;
         &lt;md-content&gt;
            &lt;h4&gt;Standard Date Picker&lt;/h4&gt;
            &lt;md-datepicker 
               ng-model = &quot;myDate&quot; 
               md-placeholder = &quot;Enter date&quot;&gt;&lt;/md-datepicker&gt;
               
            &lt;h4&gt;Disabled Date Picker&lt;/h4&gt;
            &lt;md-datepicker 
               ng-model = &quot;myDate&quot; 
               md-placeholder = &quot;Enter date&quot; 
               disabled&gt;&lt;/md-datepicker&gt;
               
            &lt;h4&gt;Date-picker with min and max dates&lt;/h4&gt;
            &lt;md-datepicker 
               ng-model = &quot;myDate&quot; 
               md-placeholder = &quot;Enter date&quot;
               md-min-date = &quot;minDate&quot; 
               md-max-date = &quot;maxDate&quot;&gt;&lt;/md-datepicker&gt;
               
            &lt;h4&gt;Date-picker with only selectable weekends&lt;/h4&gt;
            &lt;md-datepicker 
               ng-model = &quot;myDate&quot; 
               md-placeholder = &quot;Enter date&quot;
               md-date-filter = &quot;onlyWeekendsPredicate&quot;&gt;&lt;/md-datepicker&gt;
               
            &lt;h4&gt;Date-picker with only selectable weekends in a range&lt;/h4&gt;
            &lt;md-datepicker 
               ng-model = &quot;myDate&quot; 
               md-placeholder = &quot;Enter date&quot;
               md-min-date = &quot;minDate&quot; 
               md-max-date = &quot;maxDate&quot;
               md-date-filter = &quot;onlyWeekendsPredicate&quot;&gt;&lt;/md-datepicker&gt;
               
            &lt;h4&gt;Date-picker with validation mesages&lt;/h4&gt;
            &lt;form name = &quot;myForm&quot;&gt;
               &lt;md-datepicker 
                  name = &quot;dateField&quot; 
                  ng-model = &quot;myDate&quot; 
                  md-placeholder = &quot;Enter date&quot;
                  required 
                  md-min-date = &quot;minDate&quot; 
                  md-max-date = &quot;maxDate&quot;
                  md-date-filter = &quot;onlyWeekendsPredicate&quot;&gt;&lt;/md-datepicker&gt;
                  
               &lt;div class = &quot;validation-messages&quot; ng-messages = &quot;myForm.dateField.$error&quot;&gt;
                  &lt;div ng-message = &quot;valid&quot;&gt;The entered value is not a date!&lt;/div&gt;
                  &lt;div ng-message = &quot;required&quot;&gt;This date is required!&lt;/div&gt;
                  &lt;div ng-message = &quot;mindate&quot;&gt;Date is too early!&lt;/div&gt;
                  &lt;div ng-message = &quot;maxdate&quot;&gt;Date is too late!&lt;/div&gt;
                  &lt;div ng-message = &quot;filtered&quot;&gt;Only weekends are allowed!&lt;/div&gt;
               &lt;/div&gt;
            &lt;/form&gt;
            
         &lt;/md-content&gt;
      &lt;/div&gt;
   &lt;/body&gt;
&lt;/html&gt;</pre>
                                       </p>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/2otJ04/python-3-cgi-environment'>Python 3 cgi Environment</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/python3_programming_examples/'><span>python3</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">#!/usr/bin/python

import os

print (&quot;Content-type: text/html\r\n\r\n&quot;);
print (&quot;&lt;font size=+1&gt;Environment&lt;/font&gt;&lt;\br&gt;&quot;);
for param in os.environ.keys():
   print (&quot;&lt;b&gt;%20s&lt;/b&gt;: %s&lt;\br&gt;&quot; % (param, os.environ[param]))</pre>
                                       </p>
                                    </div>

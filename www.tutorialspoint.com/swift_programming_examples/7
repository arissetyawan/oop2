                                                <div class="clsQuestionSection">      
                            <div class="">   
                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/Iaxg0u/querying-and-setting-properties'>Querying and Setting Properties</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">struct StudMarks {
   static let markCount = 97
   static var totalCount = 0
   
   var InternalMarks: Int = 0 {
      didSet {
         if InternalMarks &gt; StudMarks.markCount {
            InternalMarks = StudMarks.markCount
         }
         if InternalMarks &gt; StudMarks.totalCount {
            StudMarks.totalCount = InternalMarks
         }
      }
   }
}

var stud1Mark1 = StudMarks()
var stud1Mark2 = StudMarks()

stud1Mark1.InternalMarks = 98
print(stud1Mark1.InternalMarks)

stud1Mark2.InternalMarks = 87
print(stud1Mark2.InternalMarks)</pre>
                                       </p>
                                    </div>
                                    <div class="clear"></div>
                                    <div class="qa_social_links"> 
                                       <div class="">
                                       <div class="qa_links">                                           

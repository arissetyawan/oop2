                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/pNBxWl/computed-properties-as-property-observers'>Computed Properties as Property Observers</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class Samplepgm {
   var counter: Int = 0 {
      willSet(newTotal){
         print(&quot;Total Counter is: \(newTotal)&quot;)
      }
      
      didSet {
         if counter &gt; oldValue {
            print(&quot;Newly Added Counter \(counter - oldValue)&quot;)
         }
      }
   }
}

let NewCounter = Samplepgm()
NewCounter.counter = 100
NewCounter.counter = 800</pre>
                                       </p>
                                    </div>

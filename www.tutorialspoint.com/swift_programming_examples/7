                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/zcGe06/computed-properties-as-read-only-properties'>Computed Properties as Read-Only Properties</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class film {
   var head = &quot;&quot;
   var duration = 0.0
   var metaInfo: [String:String] {
      return [
         &quot;head&quot;: self.head,
         &quot;duration&quot;:&quot;\(self.duration)&quot;
      ]
   }
}

var movie = film()
movie.head = &quot;Swift 4 Properties&quot;
movie.duration = 3.09

print(movie.metaInfo[&quot;head&quot;]!)
print(movie.metaInfo[&quot;duration&quot;]!)</pre>
                                       </p>
                                    </div>

                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/5PWSPE/computed-properties'>Computed Properties</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class sample {
   var no1 = 0.0, no2 = 0.0
   var length = 300.0, breadth = 150.0

   var middle: (Double, Double) {
      get {
         return (length / 2, breadth / 2)
      }
      
      set(axis){
         no1 = axis.0 - (length / 2)
         no2 = axis.1 - (breadth / 2)
      }
   }
}

var result = sample()
print(result.middle)
result.middle = (0.0, 10.0)

print(result.no1)
print(result.no2)</pre>
                                       </p>
                                    </div>

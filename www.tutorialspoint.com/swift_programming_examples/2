                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/jOonDg/protocol-composition'>Protocol Composition</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">protocol stname {
   var name: String { get }
}
protocol stage {
   var age: Int { get }
}
struct Person: stname, stage {
   var name: String
   var age: Int
}
func print(celebrator: stname &amp; stage) {
   print(&quot;\(celebrator.name) is \(celebrator.age) years old&quot;)
}
let studname = Person(name: &quot;Priya&quot;, age: 21)
print(studname)

let stud = Person(name: &quot;Rehan&quot;, age: 29)
print(stud)

let student = Person(name: &quot;Roshan&quot;, age: 19)
print(student)</pre>
                                       </p>

                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/diM4Zf/nested-types'>Nested Types</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">extension Int {
   enum calc {
      case add
      case sub
      case mult
      case div
      case anything
   }
   var print: calc {
      switch self {
         case 0:
            return .add
         case 1:
            return .sub
         case 2:
            return .mult
         case 3:
            return .div
         default:
            return .anything
      }
   }
}

func result(numb: [Int]) {
   for i in numb {
      switch i.print {
         case .add:
            print(&quot; 10 &quot;)
         case .sub:
            print(&quot; 20 &quot;)
         case .mult:
            print(&quot; 30 &quot;)
         case .div:
            print(&quot; 40 &quot;)
         default:
            print(&quot; 50 &quot;)
      }
   }
}
result(numb: [0, 1, 2, 3, 4, 7])</pre>
                                       </p>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/pqtRco/weak-reference-program'>Weak Reference Program</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class module {
   let name: String
   init(name: String) { self.name = name }
   var sub: submodule?
   deinit { print(&quot;\(name) Is The Main Module&quot;) }
}

class submodule {
   let number: Int
   init(number: Int) { self.number = number }
   weak var topic: module?

   deinit { print(&quot;Sub Module with its topic number is \(number)&quot;) }
}

var toc: module?
var list: submodule?
toc = module(name: &quot;ARC&quot;)
list = submodule(number: 4)
toc!.sub = list
list!.topic = toc

toc = nil
list = nil</pre>
                                       </p>

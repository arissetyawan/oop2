                                        <h2>  
                                        <a href='/programming_example/6mp89Y/weak-and-unowned-references'>Weak and Unowned References</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class HTMLElement {
   let module: String
   let text: String?
   
   lazy var asHTML: () -&gt; String = {
      [unowned self] in
      if let text = self.text {
         return &quot;&lt;\(self.module)&gt;\(text)&lt;/\(self.module)&gt;&quot;
      } else {
         return &quot;&lt;\(self.module) /&gt;&quot;
      }
   }
   init(module: String, text: String? = nil) {
      self.module = module
      self.text = text
   }
   deinit {
      print(&quot;\(module) the deinit()&quot;)
   }
}

var paragraph: HTMLElement? = HTMLElement(module: &quot;Inside&quot;, text: &quot;ARC Weak References&quot;)
print(paragraph!.asHTML())
paragraph = nil</pre>

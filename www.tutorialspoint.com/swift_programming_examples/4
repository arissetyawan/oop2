                                        <h2>  
                                        <a href='/programming_example/o29EmD/arc-strong-reference-cycles'>ARC Strong Reference Cycles</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class studmarks {
   let name: String
   var stud: student?
   
   init (name: String) {
      print(&quot;Initializing: \(name)&quot;)
      self.name = name
   }
   deinit {
      print(&quot;Deallocating: \(self.name)&quot;)
   }
}

class student {
   let name: String
   var strname: studmarks?
   
   init (name: String) {
      print(&quot;Initializing: \(name)&quot;)
      self.name = name
   }
   deinit {
      print(&quot;Deallocating: \(self.name)&quot;)
   }
}

var shiba: studmarks?
var mari: student?

shiba = studmarks(name: &quot;Swift 4&quot;)
mari = student(name: &quot;ARC&quot;)

shiba!.stud = mari
mari!.strname = shiba</pre>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/WFpOXw/init-failable-initializer'>Init Failable Initializer</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">struct studrecord {
let stname: String

   init!(stname: String) {
      if stname.isEmpty {return nil }
      self.stname = stname
   }
}
let stmark = studrecord(stname: &quot;Swing&quot;)

if let name = stmark {
   print(&quot;Student name is specified&quot;)
}

let blankname = studrecord(stname: &quot;&quot;)

if blankname == nil {
   print(&quot;Student name is left blank&quot;)
}</pre>
                                       </p>
                                    </div>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/0cfrCT/required-initializers'>Required Initializers</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class classA {
   required init() {
      var a = 10
      print(a)
   }
}

class classB: classA {
   required init() {
      var b = 30
      print(b)
   }
}

let res = classA()
let print = classB()</pre>
                                       </p>
                                    </div>
                                    <div class="clear"></div>

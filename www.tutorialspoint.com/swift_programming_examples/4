                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/O8nUkv/failable-initializers-for-classes'>Failable Initializers for Classes</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/swift_programming_examples/'><span>swift</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">class studrecord {
   let studname: String!
   init?(studname: String) {
      self.studname = studname
      if studname.isEmpty { return nil }
   }
}

if let stname = studrecord(studname: &quot;Failable Initializers&quot;) {
   print(&quot;Module is \(stname.studname)&quot;)
}</pre>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/rrdeU1/vb-net-passing-arrays-as-function-arguments'>VB.Net Passing Arrays as Function Arguments</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/vb.net_programming_examples/'><span>vb.net</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">Module arrayParameter
   Function getAverage(ByVal arr As Integer(), ByVal size As Integer) As Double
      'local variables
       Dim i As Integer
       Dim avg As Double
       Dim sum As Integer = 0
       For i = 0 To size - 1
           sum += arr(i)
       Next i
       avg = sum / size
       Return avg
    End Function
    Sub Main()
        ' an int array with 5 elements '
        Dim balance As Integer() = {1000, 2, 3, 17, 50}
        Dim avg As Double
        'pass pointer to the array as an argument 
        avg = getAverage(balance, 5)
        ' output the returned value '
        Console.WriteLine(&quot;Average value is: {0} &quot;, avg)
        Console.ReadLine()
    End Sub
End Module</pre>
                                       </p>
                                    </div>

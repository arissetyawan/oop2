                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/iUpI29/vb-net-function-returning-a-value'>VB.Net Function Returning a Value</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/vb.net_programming_examples/'><span>vb.net</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">Module myfunctions
   Function FindMax(ByVal num1 As Integer, ByVal num2 As Integer) As Integer
      ' local variable declaration */
      Dim result As Integer
      If (num1 &gt; num2) Then
          result = num1
      Else
          result = num2
      End If
      FindMax = result
   End Function
   Sub Main()
      Dim a As Integer = 100
      Dim b As Integer = 200
      Dim res As Integer
      res = FindMax(a, b)
      Console.WriteLine(&quot;Max value is : {0}&quot;, res)
      Console.ReadLine()
   End Sub
End Module</pre>
                                       </p>
                                    </div>

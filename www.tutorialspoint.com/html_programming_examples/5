      &lt;section class = &quot;section&quot;&gt;
         &lt;div class = &quot;container&quot;&gt;
            &lt;span class = &quot;title&quot;&gt;
            Tags
            &lt;/span&gt;&lt;br&gt;&lt;br&gt;
            &lt;span class = &quot;is-size-5&quot;&gt;List of tags&lt;/span&gt;&lt;br&gt;&lt;br&gt;
            &lt;div class = &quot;tags&quot;&gt;
               &lt;span class = &quot;tag&quot;&gt;India&lt;/span&gt;
               &lt;span class = &quot;tag&quot;&gt;Srilanka&lt;/span&gt;

               &lt;div class = &quot;control has-icons-left&quot;&gt;
                  &lt;div class = &quot;select&quot;&gt;
                     &lt;select&gt;
                        &lt;option&gt;Option One&lt;/option&gt;
                        &lt;option&gt;Option Two&lt;/option&gt;

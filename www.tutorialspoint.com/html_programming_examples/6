	<script>
	$(document).ready(function(){
		$('.btnEmbed').click(function(e){
			e.stopPropagation();
			e.preventDefault();
			$('.divEmbed').hide();
			$(this).next().show();
		});
		$('.divEmbed').click(function(e){
			e.stopPropagation();
		});
		$(document).click(function(e){
			$('.divEmbed').hide();
		});
	});
	</script>


                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/HBbEWk/d-accessing-union-members-modified'>D Accessing Union Members Modified</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/d_programming_examples/'><span>d</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">import std.stdio;

union Data { 
   int i; 
   float f; 
   char str[13]; 
};  
void main( ) { 
   Data data; 
   writeln( &quot;size of : &quot;, data.sizeof);  
   
   data.i = 10; 
   writeln( &quot;data.i : &quot;, data.i); 
   
   data.f = 220.5; 
   writeln( &quot;data.f : &quot;, data.f);  
   
   data.str = &quot;D Programming&quot;.dup; 
   writeln( &quot;data.str : &quot;, data.str); 
}</pre>

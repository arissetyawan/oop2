                                        <h2>  
                                        <a href='/programming_example/9MIiar/go-global-variables'>Go Global Variables</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/golang_programming_examples/'><span>golang</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package main

import &quot;fmt&quot;
 
/* global variable declaration */
var g int
 
func main() {
   /* local variable declaration */
   var a, b int

   /* actual initialization */
   a = 10
   b = 20
   g = a + b

   fmt.Printf(&quot;value of a = %d, b = %d and g = %d\n&quot;, a, b, g)
}</pre>

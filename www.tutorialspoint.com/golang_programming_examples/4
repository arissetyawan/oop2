                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/0fgVN1/go-functions-as-values'>Go functions as values</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/golang_programming_examples/'><span>golang</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package main

import (
   &quot;fmt&quot; 
   &quot;math&quot; 
)

func main(){
   /* declare a function variable */
   getSquareRoot := func(x float64) float64 {
      return math.Sqrt(x)
   }

   /* use the function */
   fmt.Println(getSquareRoot(9))
}</pre>

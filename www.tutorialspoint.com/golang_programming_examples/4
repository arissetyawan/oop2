                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/tZxgtZ/go-call-by-reference'>Go Call by reference</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/golang_programming_examples/'><span>golang</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">package main

import &quot;fmt&quot;

func main() {
   /* local variable definition */
   var a int = 100
   var b int = 200

   fmt.Printf(&quot;Before swap, value of a : %d\n&quot;, a )
   fmt.Printf(&quot;Before swap, value of b : %d\n&quot;, b )

   /* calling a function to swap the values */
   swap(a, b)

   fmt.Printf(&quot;After swap, value of a : %d\n&quot;, a )
   fmt.Printf(&quot;After swap, value of b : %d\n&quot;, b )
}
func swap(x, y int) int {
   var temp int

   temp = x /* save the value of x */
   x = y    /* put y into x */
   y = temp /* put temp into y */

   return temp;
}</pre>
                                       </p>
                                    </div>
                                    <div class="clear"></div>

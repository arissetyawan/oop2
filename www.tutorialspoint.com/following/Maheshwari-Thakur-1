<script>
    $(document).ready(function () {
        getTopWriters();
        function getTopWriters() {
            $.ajax({
                url: baseurl + "questions/ajax/ajaxGetTopWriters.php",
                method: "POST",
                success: function (data) {
                    $('#divTopWriters').html(data);
                }
            });
        }
    });
</script>

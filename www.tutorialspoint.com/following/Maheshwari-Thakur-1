<script>
    var start_from = 20;
    var v = false;
    var lastFlag = false;
    var uid = $('#uid').val();
    $(document).ready(function () {
        $(window).scroll(function () {
            $('.wrap_loader').css('height', $(document).height() - 40);
            $('.search_bg_mask').css('height', $(document).height() - 40);
            if (Math.round($(window).scrollTop()) == Math.round($(document).height() - $(window).height())) {
                if (lastFlag == false) {
                    getUsers(uid, start_from, 20);
                    start_from = start_from + 20;
                }
            }
        });
    });
    function getUsers(uid, start_from, limit_val) {
        $('#loader_image').show();
        $.ajax({
            url: baseurl + "questions/ajax/ajaxGetUserFollowing.php",
            method: "POST",
            data: {'uid': uid, 'start_from': start_from, "limit_val": limit_val}
        }).done(function (returnData) {
            if (returnData == '') {
                lastFlag = true;
                $('#user_profile').append("<div class='admin_qa_widget'>No more profiles available</div>").fadeIn('slow');
                $('#loader_image').hide();
            }
            $('#user_profile').append(returnData).fadeIn('slow');
            progressively.init({});
            $('#loader_image').hide();
        });
    }
</script>


                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/TJZyZc/java-method-getfirst'>Java Method getFirst() </a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/java8_programming_examples/'><span>java8</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">import java.util.*;

public class LinkedListDemo {
   public static void main(String[] args) {
      LinkedList list = new LinkedList();
      list.add(&quot;Hello&quot;);
      list.add(2);
      list.add(&quot;Chocolate&quot;);
      list.add(&quot;10&quot;);
      System.out.println(&quot;LinkedList:&quot; + list);
      System.out.println(&quot;First Element :&quot; + list.getFirst());
   }
}</pre>

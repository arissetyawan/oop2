                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/SGMfTd/fortran-formatted-print'>Fortran  Formatted Print</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/fortran_programming_examples/'><span>fortran</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">program formattedPrint
implicit none

   real :: c = 1.2786456e-9, d = 0.1234567e3 
   integer :: n = 300789, k = 45, i = 2
   character (len=15) :: str=&quot;Tutorials Point&quot;
   
   print &quot;(i6)&quot;, k 
   print &quot;(i6.3)&quot;, k 
   print &quot;(3i10)&quot;, n, k, i 
   print &quot;(i10,i3,i5)&quot;, n, k, i 
   print &quot;(a15)&quot;,str 
   print &quot;(f12.3)&quot;, d
   print &quot;(e12.4)&quot;, c 
   print '(/,3x,&quot;n = &quot;,i6, 3x, &quot;d = &quot;,f7.4)', n, d
   
end program formattedPrint</pre>
                                       </p>

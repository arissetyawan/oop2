                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/OTBKM5/fortran-calling-a-subroutine'>Fortran Calling a Subroutine</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/fortran_programming_examples/'><span>fortran</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">program calling_func
implicit none

   real :: a, b
   a = 2.0
   b = 3.0
   
   Print *, &quot;Before calling swap&quot;
   Print *, &quot;a = &quot;, a
   Print *, &quot;b = &quot;, b
   
   call swap(a, b)
   
   Print *, &quot;After calling swap&quot;
   Print *, &quot;a = &quot;, a
   Print *, &quot;b = &quot;, b
   
end program calling_func


subroutine swap(x, y) 
implicit none

   real :: x, y, temp   
   
   temp = x  
   x = y 
   y = temp  
   
end subroutine swap</pre>
                                       </p>

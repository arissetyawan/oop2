                                <div class="qa_widget qa_widget_popup">                                
                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/yKSqrO/fortran-pointer-example'>Fortran pointer Example</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/fortran_programming_examples/'><span>fortran</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">program pointerExample
implicit none

   integer, pointer :: a, b
   integer, target :: t
   integer :: n
   
   t= 1
   a=&gt;t
   t = 2
   b =&gt; t
   n = a + b
   
   Print *, a, b, t, n 
   
end program pointerExample</pre>
                                       </p>
                                    </div>
                                    <div class="clear"></div>
                                    <div class="qa_social_links"> 
                                       <div class="">
                                       <div class="qa_links">                                           
                                          <a href="http://tpcg.io/yKSqrO" target="_blank" rel="nofollow" class="demo"><button type="button" class="question_btn"><i class="fa fa-external-link"></i> Live Demo</button></a> 
                                                                                    <button type="button" class="btnEmbed question_btn"><i class="fa fa-code"></i> Embed URL </button> 
                                          <div style="display:none;" class="divEmbed arrow_boxx embed-box">
                                             <input type="text" value="http://tpcg.io/yKSqrO" />
                                          </div>
                                       </div>
                                       </div>
                                    </div>
                                    <div class="clear"></div>
                                </div>

                                    <div class="qa_post" >
                                        <h2>  
                                        <a href='/programming_example/XJY3lw/fortran-allocate-statement'>Fortran allocate statement</a>                                       </h2>
                                        <div class="qa_category">
                                       <a href='/fortran_programming_examples/'><span>fortran</span></a>                                       </div>  
                                       <p class="qa_post_desc">
                                          <pre class="prettyprint" style="">program pointerExample
implicit none

   integer, pointer :: p1
   allocate(p1)
   
   p1 = 1
   Print *, p1
   
   p1 = p1 + 4
   Print *, p1
   
end program pointerExample</pre>
                                       </p>
